<launch>
	<!-- VIDEO SOURCE -->
	<include file="$(find-pkg-share ros_deep_learning)/launch/video_source.ros2.launch"/>

    <!-- DETECTOR -->
    <include file="$(find-pkg-share rrbot_cam_object_tracker)/launch/detector.launch.py"/>

	<!-- VIDEO OUTPUT -->
	<include file="$(find-pkg-share ros_deep_learning)/launch/video_output.ros2.launch">
		<arg name="topic" value="/debug_image"/>
        <arg name="output" value="webrtc://@:8554/output"/>
 	</include>

</launch>